<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="sample.mapper.sqlite.CoMapper">
	
	<select id="getSoliderInfo" parameterType="String" resultType="sample.model.SoliderInfo">
		SELECT *
		  FROM SOLIDER
		 WHERE SOLIDER_ID = #{soliderId}
	</select>
	
</mapper>